  rdfs:comment "This property defines the number of time the alarm should be repeated, after the initial trigger. Inspired by RFC 2445 sec. 4.8.6.2" ;
  rdfs:domain ncal:Alarm ;
  rdfs:label "repeat" ;
  rdfs:range xsd:integer ;
