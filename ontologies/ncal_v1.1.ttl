ncal:Freebusy
  rdf:type rdfs:Class ;
  rdfs:comment "Provide a grouping of component properties that describe either a request for free/busy time, describe a response to a request for free/busy time or describe a published set of busy time." ;
  rdfs:label "Freebusy" ;
